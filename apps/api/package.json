{
  "name": "api",
  "version": "0.0.0",
  "author": "Ben Cochran <bcochran@gmail.com>",
  "license": "MIT",
  "private": true,
  "exports": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn prisma generate && tsc",
    "start": "node ./dist/index.js",
    "dev": "concurrently -n server,prisma,grapql \"yarn dev:server\" \"yarn dev:prisma\" \"yarn dev:graphql\"",
    "dev:server": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e ts -w src -w .env",
    "dev:prisma": "yarn prisma generate --watch",
    "dev:graphql": "nodemon --exec \"yarn graphql:generate\" -e ts -w src/graphql -w scripts/generate-graphql-schema.ts",
    "db:generate": "yarn prisma generate",
    "graphql:generate": "tsx ./scripts/generate-graphql-schema.ts",
    "release": "yarn prisma migrate deploy"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/node": "^20.2.3",
    "esbuild-register": "^3.4.2",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.341.0",
    "@graphql-yoga/plugin-graphql-sse": "^1.5.1",
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-prisma": "^3.51.1",
    "@pothos/plugin-relay": "^3.43.0",
    "@prisma/client": "^4.14.1",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "google-libphonenumber": "^3.2.32",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.22.0",
    "graphql-yoga": "^3.9.1",
    "p-queue": "^6.6.2",
    "prisma": "^4.14.1",
    "rss-parser": "^3.13.0",
    "twilio": "^4.11.1",
    "winston": "^3.8.2"
  }
}
